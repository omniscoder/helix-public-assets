{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://omniscoder.github.io/Helix/schemas/outputs/gate1.schema.json",
  "title": "Gate1Outputs v0",
  "description": "Gate 1 outputs: context/dataset validated, still non-probabilistic.",
  "type": "object",
  "additionalProperties": false,
  "required": ["schema_version", "gate_level", "outcome_value_label", "outcomes"],
  "properties": {
    "schema_version": {"type": "string", "const": "gate1_v0"},
    "gate_level": {"type": "integer", "const": 1},
    "outcome_value_label": {"type": "string", "const": "score"},
    "outcomes": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["outcome_id", "score", "rank"],
        "properties": {
          "outcome_id": {"type": "string", "minLength": 1},
          "rank": {"type": "integer", "minimum": 1},
          "score": {"type": "number"},
          "label": {"type": ["string", "null"]}
        }
      }
    },
    "baseline_metrics": {
      "type": ["object", "null"],
      "additionalProperties": false,
      "required": ["baseline_score"],
      "properties": {
        "baseline_score": {"type": "number"}
      }
    }
  }
}

