{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "helix_calibration_preview_v1.json",
  "title": "Helix Calibration Preview v1",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "schema",
    "profileId",
    "modality",
    "pPred",
    "pCal",
    "includedResults",
    "skippedResults"
  ],
  "properties": {
    "schema": {"const": "helix_calibration_preview_v1"},
    "profileId": {"type": "string"},
    "modality": {"type": "string", "enum": ["cut", "prime"]},
    "pPred": {"type": "number"},
    "pObs": {"type": ["number", "null"]},
    "pCal": {"type": "number"},
    "delta": {"type": "number"},
    "includedResults": {"type": "array", "items": {"type": "string"}},
    "skippedResults": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "resultsId": {"type": "string"},
          "reason": {"type": "string"}
        }
      }
    }
  }
}
